import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);          // Scanner and System should be capitalized
        System.out.println("Enter a number ");
        int n = sc.nextInt();                          // nextInt() capitalization fixed
        
        int i = 1;                                    // Initialize i before using in while loop
        while (i <= n) {
            if (i % 2 == 0) {
                System.out.println(i);
            }
            i++;
        }
        sc.close();                                   // Close scanner to avoid resource leak
    }
}
